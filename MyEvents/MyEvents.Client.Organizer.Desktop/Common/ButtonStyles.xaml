<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero">

    <Style x:Key="MainDesktopStyle" TargetType="{x:Type RadioButton}">
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="FontSize" Value="18"/>
        <Setter Property="FontFamily" Value="Segoe UI"/>
        <Setter Property="Foreground" Value="{StaticResource NormalTextForegroundBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type RadioButton}">
                    <StackPanel Orientation="Horizontal" Background="#00FFFFFF" Margin="0,0,20,0">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CheckStates">
                                <VisualState x:Name="Checked"/>
                                <VisualState x:Name="Unchecked">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="UnselectedText"/>
                                        <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="UnselectedImage"/>
                                        <DoubleAnimation Duration="0" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="SelectedText"/>
                                        <DoubleAnimation Duration="0" To="0" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="SelectedImage"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid>
                            <TextBlock x:Name="SelectedText" Text="{TemplateBinding Content}" VerticalAlignment="Center"
                                       FontFamily="Segoe UI" FontSize="18" Foreground="{StaticResource NormalTextForegroundBrush}" Opacity="1"/>
                            <TextBlock x:Name="UnselectedText" Text="{TemplateBinding Content}" VerticalAlignment="Center"
                                       FontFamily="Segoe UI" FontSize="18" Foreground="#FF2b4f78" Opacity="0"/>
                        </Grid>
                        <Grid Width="36" Height="36" Margin="10,0,0,0">
                            <Image x:Name="SelectedImage" Source="/Resources/Images/MainDesktop_enabled.png" Opacity="1"/>
                            <Image x:Name="UnselectedImage" Source="/Resources/Images/MainDesktop_disabled.png" Opacity="0"/>
                        </Grid>
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

